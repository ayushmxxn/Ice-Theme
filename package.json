{
  "name": "blazetheme",
  "displayName": "Ice Theme",
  "version": "2.1.1",
  "main": "extension/extension.js",
  "activationEvents": ["onStartupFinished" ],
  "publisher": "AyushmaanSingh",
  "description": "Icy tones create a serene workspace.",
  "categories": [
    "Themes"
  ],
  "keywords": [
    "Ice Theme",
    "ice theme",
    "icy tones",
    "cool theme",
    "dark theme",
    "VS Code",
    "theme",
    "Dark",
    "Blue"
  ],
  "engines": {
    "vscode": "^1.88.0"
  },
  "icon": "Ice/extension/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ayushmaan0101/Ice-Theme"
  },
  "contributes": {
    "themes": [
      {
        "label": "Ice",
        "uiTheme": "vs-dark",
        "path": "./Ice/themes/ice-color-theme.json"
      },
      {
        "label": "Blaze",
        "uiTheme": "vs-dark",
        "path": "./Blaze/themes/blaze-color-theme.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "My Extension Settings",
      "properties": {
        "editor.fontFamily": {
          "type": "string",
          "default": "Monaco, Consolas, 'Courier New', monospace",
          "description": "Specifies the font family for the editor."
        }
      }
    }
  }
}
